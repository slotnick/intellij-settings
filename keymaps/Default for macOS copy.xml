<keymap version="1" name="Default for macOS copy" parent="Mac OS X 10.5+">
  <action id="CollapseAllRegions">
    <keyboard-shortcut first-keystroke="shift meta subtract" />
  </action>
  <action id="DisableInspection">
    <keyboard-shortcut first-keystroke="alt d" />
  </action>
  <action id="EditorDecreaseFontSize">
    <keyboard-shortcut first-keystroke="shift meta minus" />
  </action>
  <action id="EditorIncreaseFontSize">
    <keyboard-shortcut first-keystroke="shift meta equals" />
  </action>
  <action id="EditorResetFontSize">
    <keyboard-shortcut first-keystroke="shift meta 0" />
  </action>
  <action id="ExpandAllRegions">
    <keyboard-shortcut first-keystroke="shift meta add" />
  </action>
  <action id="MoveTabRight">
    <keyboard-shortcut first-keystroke="alt dead_grave" />
    <keyboard-shortcut first-keystroke="ctrl alt close_bracket" />
  </action>
  <action id="Unsplit">
    <keyboard-shortcut first-keystroke="ctrl alt open_bracket" />
  </action>
</keymap>